{
  "name": "koa_casn",
  "version": "1.0.0",
  "description": "koa+casbin+prisma+pg(混合RBAC+ABAC进行多角色、多租户系统)",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "migrate": "prisma migrate dev --name init",
    "generate": "prisma generate",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "test:unit": "jest --config jest.unit.config.js",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:watch": "jest --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --config jest.unit.config.js",
    "test:debug:e2e": "node --inspect-brk ./node_modules/.bin/jest --runInBand --config jest.e2e.config.js",
    "test:debug:watch": "node --inspect-brk ./node_modules/.bin/jest --watch --config jest.unit.config.js",
    "test:debug:coverage": "node --inspect-brk ./node_modules/.bin/jest --coverage --config jest.unit.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@prisma/client": "^6.10.1",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "casbin": "^5.38.0",
    "casbin-pg-adapter": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dayjs": "^1.11.13",
    "dotenv": "^17.1.0",
    "etcd3": "^1.1.2",
    "got": "^14.4.7",
    "i18n": "^0.15.1",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "koa": "^3.0.0",
    "koa-bodyparser": "^4.4.1",
    "koa-helmet": "^8.0.1",
    "koa-i18n": "^2.1.0",
    "koa-router": "^13.0.1",
    "koa-static": "^5.0.0",
    "koa2-swagger-ui": "^5.11.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "svg-captcha": "^1.4.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-koa": "^0.0.1",
    "typeorm": "^0.3.25",
    "typeorm-adapter": "^1.7.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-router": "^7.4.8",
    "jest": "^30.0.2",
    "prisma": "^6.10.1",
    "ts-jest": "^29.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
